{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/equipment/batch.ts": {
      "maxDuration": 30
    },
    "api/test-datascope.ts": {
      "maxDuration": 20
    },
    "api/debug.ts": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}